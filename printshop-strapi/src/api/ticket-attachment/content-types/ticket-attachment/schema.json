{
  "kind": "collectionType",
  "collectionName": "ticket_attachments",
  "info": {
    "singularName": "ticket-attachment",
    "pluralName": "ticket-attachments",
    "displayName": "Ticket Attachment"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": false
    }
  },
  "attributes": {
    "ticket": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::support-ticket.support-ticket",
      "inversedBy": "attachments"
    },
    "comment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ticket-comment.ticket-comment",
      "inversedBy": "attachments"
    },
    "fileName": {
      "type": "string",
      "required": true
    },
    "fileUrl": {
      "type": "string",
      "required": true
    },
    "fileSize": {
      "type": "integer"
    },
    "mimeType": {
      "type": "string"
    }
  }
}
