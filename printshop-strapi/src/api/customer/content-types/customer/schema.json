{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": false
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "unique": true
    },
    "jobs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::job.job",
      "mappedBy": "customer"
    },
    "quotes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::quote.quote",
      "mappedBy": "customer"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "customer"
    }
  }
}
