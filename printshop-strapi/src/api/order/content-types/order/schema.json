{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": "Customer orders"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "orderNumber": {
      "type": "string",
      "required": true
    },
    "orderNickname": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "QUOTE",
        "QUOTE_SENT",
        "QUOTE_APPROVED",
        "IN_PRODUCTION",
        "COMPLETE",
        "READY_FOR_PICKUP",
        "SHIPPED",
        "PAYMENT_NEEDED",
        "INVOICE_PAID",
        "CANCELLED"
      ],
      "default": "QUOTE"
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::customer.customer",
      "inversedBy": "orders"
    },
    "lineItems": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::line-item.line-item",
      "mappedBy": "order"
    },
    "totalAmount": {
      "type": "decimal",
      "default": 0
    },
    "amountPaid": {
      "type": "decimal",
      "default": 0
    },
    "amountOutstanding": {
      "type": "decimal",
      "default": 0
    },
    "salesTax": {
      "type": "decimal",
      "default": 0
    },
    "discount": {
      "type": "decimal",
      "default": 0
    },
    "fees": {
      "type": "decimal",
      "default": 0
    },
    "dueDate": {
      "type": "date"
    },
    "customerDueDate": {
      "type": "date"
    },
    "productionDueDate": {
      "type": "date"
    },
    "invoiceDate": {
      "type": "date"
    },
    "paymentDueDate": {
      "type": "date"
    },
    "deliveryMethod": {
      "type": "enumeration",
      "enum": [
        "pickup",
        "ship",
        "delivery",
        "other"
      ]
    },
    "notes": {
      "type": "text"
    },
    "productionNotes": {
      "type": "text"
    },
    "customerPO": {
      "type": "string"
    },
    "approved": {
      "type": "boolean",
      "default": false
    },
    "approvedBy": {
      "type": "string"
    },
    "approvedAt": {
      "type": "datetime"
    },
    "items": {
      "type": "json"
    },
    "printavoId": {
      "type": "string",
      "unique": true
    },
    "printavoCustomerId": {
      "type": "string"
    },
    "visualId": {
      "type": "string"
    },
    "publicHash": {
      "type": "string"
    },
    "jobs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::job.job",
      "mappedBy": "order"
    },
    "billingAddress": {
      "type": "json"
    },
    "shippingAddress": {
      "type": "json"
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment",
      "mappedBy": "order"
    }
  }
}
