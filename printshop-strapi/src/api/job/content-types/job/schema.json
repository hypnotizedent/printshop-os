{
  "kind": "collectionType",
  "collectionName": "jobs",
  "info": {
    "singularName": "job",
    "pluralName": "jobs",
    "displayName": "Job"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": false
    }
  },
  "attributes": {
    "jobNumber": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "PendingArtwork",
        "Pending",
        "Ready",
        "InProduction",
        "Complete",
        "Cancelled"
      ],
      "default": "PendingArtwork",
      "required": true
    },
    "paymentStatus": {
      "type": "enumeration",
      "enum": [
        "Paid",
        "Unpaid",
        "Partial"
      ]
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::customer.customer",
      "inversedBy": "jobs"
    },
    "dueDate": {
      "type": "date",
      "required": true
    },
    "productionNotes": {
      "type": "richtext"
    },
    "customerNotes": {
      "type": "richtext"
    },
    "totalAmount": {
      "type": "decimal"
    },
    "amountPaid": {
      "type": "decimal"
    },
    "mockupUrls": {
      "type": "json"
    },
    "artFileUrls": {
      "type": "json"
    },
    "printavoId": {
      "type": "string"
    },
    "order": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order",
      "inversedBy": "job"
    },
    "quote": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::quote.quote"
    },
    "created_at_timestamp": {
      "type": "datetime"
    },
    "priority": {
      "type": "enumeration",
      "enum": [
        "Low",
        "Normal",
        "High",
        "Rush"
      ],
      "default": "Normal"
    },
    "assignedTo": {
      "type": "string"
    },
    "estimatedHours": {
      "type": "decimal"
    },
    "actualHours": {
      "type": "decimal"
    },
    "completionPercentage": {
      "type": "integer",
      "default": 0,
      "min": 0,
      "max": 100
    },
    "productionStartedAt": {
      "type": "datetime"
    },
    "productionCompletedAt": {
      "type": "datetime"
    },
    "qualityCheckPassed": {
      "type": "boolean",
      "default": false
    },
    "qualityCheckNotes": {
      "type": "text"
    },
    "shippingMethod": {
      "type": "string"
    },
    "trackingNumber": {
      "type": "string"
    },
    "items": {
      "type": "json"
    }
  }
}
