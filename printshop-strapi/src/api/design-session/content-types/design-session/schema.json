{
  "kind": "collectionType",
  "collectionName": "design_sessions",
  "info": {
    "singularName": "design-session",
    "pluralName": "design-sessions",
    "displayName": "Design Session",
    "description": "Customer design sessions for the online designer tool"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "customerEmail": {
      "type": "email"
    },
    "customerId": {
      "type": "string"
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::customer.customer"
    },
    "garmentType": {
      "type": "enumeration",
      "enum": [
        "t-shirt",
        "hoodie",
        "tank-top",
        "long-sleeve",
        "polo",
        "sweatshirt",
        "hat",
        "jacket"
      ],
      "default": "t-shirt",
      "required": true
    },
    "garmentColor": {
      "type": "string",
      "default": "#FFFFFF"
    },
    "garmentSize": {
      "type": "enumeration",
      "enum": [
        "XS",
        "S",
        "M",
        "L",
        "XL",
        "XXL",
        "XXXL",
        "XXXXL",
        "XXXXXL"
      ],
      "default": "M"
    },
    "printMethod": {
      "type": "enumeration",
      "enum": [
        "screen-print",
        "dtg",
        "embroidery",
        "heat-transfer",
        "sublimation"
      ],
      "default": "screen-print"
    },
    "quantity": {
      "type": "integer",
      "default": 1,
      "min": 1
    },
    "designs": {
      "type": "json",
      "required": true
    },
    "canvasData": {
      "type": "json"
    },
    "pricing": {
      "type": "json",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "draft",
        "active",
        "quoted",
        "ordered",
        "completed",
        "abandoned"
      ],
      "default": "active",
      "required": true
    },
    "productId": {
      "type": "string"
    },
    "thumbnailUrl": {
      "type": "string"
    },
    "mockupUrl": {
      "type": "string"
    },
    "notes": {
      "type": "text"
    },
    "shareToken": {
      "type": "string"
    }
  }
}
