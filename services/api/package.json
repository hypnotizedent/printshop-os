{
  "name": "@printshop-os/api",
  "version": "1.0.0",
  "description": "PrintShop OS API Service - Orders, Quotes, and Data Sync",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "demo": "ts-node src/demo.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "sync:printavo": "ts-node scripts/sync-printavo-live.ts",
    "import:historical": "ts-node scripts/run-import.ts",
    "import:batch": "ts-node scripts/run-import.ts",
    "upload:strapi": "ts-node scripts/upload-to-strapi.ts",
    "lint": "tsc --noEmit",
    "printavo:extract": "ts-node scripts/extract-printavo-v2.ts",
    "printavo:extract-complete": "ts-node scripts/extract-printavo-v2-complete.ts",
    "printavo:extract-complete:resume": "ts-node scripts/extract-printavo-v2-complete.ts --resume",
    "printavo:import": "ts-node scripts/import-printavo-v2.ts",
    "printavo:migrate": "npm run printavo:extract && npm run printavo:import",
    "printavo:download-files": "ts-node scripts/download-printavo-files.ts",
    "printavo:sync-minio": "ts-node scripts/sync-to-minio.ts",
    "printavo:full-archive": "npm run printavo:extract && npm run printavo:download-files && npm run printavo:sync-minio",
    "migrate:extract": "node scripts/printavo-extract.js",
    "migrate:extract:incremental": "node scripts/printavo-extract.js --since",
    "migrate:import": "node scripts/strapi-import.js",
    "migrate:full": "bash scripts/migrate-printavo.sh",
    "migrate:incremental": "bash scripts/migrate-printavo.sh --incremental",
    "migrate:test": "bash scripts/test-migration.sh"
  },
  "keywords": [
    "printshop",
    "api",
    "orders",
    "quotes",
    "printavo",
    "strapi"
  ],
  "author": "PrintShop OS Team",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "jest": "^29.5.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@aws-sdk/s3-request-presigner": "^3.940.0",
    "@sendgrid/mail": "^8.1.6",
    "@types/archiver": "^7.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pdfkit": "^0.17.3",
    "@types/pg": "^8.15.6",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "@types/stripe": "^8.0.416",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "archiver": "^7.0.1",
    "axios": "^1.4.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.2",
    "minio": "^7.1.3",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "redis": "^5.10.0",
    "speakeasy": "^2.0.0",
    "stripe": "^20.0.0",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "uuid": "^13.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
