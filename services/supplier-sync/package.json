{
  "name": "@mintprints/supplier-sync",
  "version": "1.0.0",
  "description": "Supplier integration and product sync service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "ts-node src/cli/test-integration.ts",
    "sync:ss": "ts-node src/cli/sync-ss-activewear.ts",
    "sync:ss:categories": "ts-node src/cli/sync-ss-activewear.ts categories",
    "sync:ss:brands": "ts-node src/cli/sync-ss-activewear.ts brands",
    "sync:ascolour": "ts-node src/cli/sync-as-colour.ts --dry-run --limit=10",
    "sync:ascolour:full": "ts-node src/cli/sync-as-colour.ts --enrich-variants --enrich-prices",
    "sync:ascolour:incremental": "ts-node src/cli/sync-as-colour.ts --enrich-variants --updated-since=$(date -u -v-7d +%Y-%m-%dT%H:%M:%S)",
    "sync:sanmar": "echo 'SanMar sync not yet implemented'",
    "sync:all": "npm run sync:ss",
    "curate": "ts-node src/cli/curate-products.ts",
    "curate:import": "ts-node src/cli/curate-products.ts import-popular",
    "curate:list": "ts-node src/cli/curate-products.ts list-top",
    "curate:sync": "ts-node src/cli/curate-products.ts sync-curated",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "axios": "^1.6.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "ioredis": "^5.3.0",
    "rate-limiter-flexible": "^3.0.0",
    "ssh2-sftp-client": "^12.0.1",
    "unzipper": "^0.12.3",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.25",
    "@types/ssh2-sftp-client": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "supplier-integration",
    "product-sync",
    "apparel",
    "mint-prints"
  ],
  "author": "Mint Prints",
  "license": "MIT"
}
