{
  "name": "@mintprints/vector-store",
  "version": "1.0.0",
  "description": "Milvus vector database service for AI-powered features",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "init:collections": "ts-node src/scripts/init-collections.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "@zilliz/milvus2-sdk-node": "^2.4.4",
    "dotenv": "^16.3.0",
    "openai": "^4.47.1",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.25",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "vector-database",
    "milvus",
    "embeddings",
    "semantic-search",
    "rag",
    "mint-prints"
  ],
  "author": "Mint Prints",
  "license": "MIT"
}
