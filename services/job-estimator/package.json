{
  "name": "@mintprints/job-estimator",
  "version": "1.0.0",
  "description": "Unified pricing and job estimation engine for Mint Prints",
  "main": "lib/estimator.ts",
  "types": "lib/estimator.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "ts-node lib/estimator.ts",
    "start": "node build/estimator.js",
    "api:dev": "ts-node lib/api-server.ts",
    "api:start": "node build/api-server.js",
    "lint": "eslint lib tests --ext .ts",
    "format": "prettier --write \"lib/**/*.ts\" \"tests/**/*.ts\""
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "pricing",
    "job-estimator",
    "mint-prints",
    "apparel",
    "embroidery",
    "screenprint",
    "estimator"
  ],
  "author": "Mint Prints",
  "license": "MIT"
}
