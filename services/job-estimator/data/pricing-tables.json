  "pricingFormulas": {
    "basePrice": {
      "formula": "(service_base + color_surcharge) * size_multiplier",
      "example": "(4.00 + 0.50) * 1.0 = 4.50 per unit"
    },
    "subtotal": {
      "formula": "base_price * quantity + setup_fee",
      "example": "4.50 * 100 + 74.28 = 556.28"
    },
    "locationPrice": {
      "formula": "subtotal * location_multiplier",
      "example": "556.28 * 1.0 = 556.28"
    },
    "rushPrice": {
      "formula": "location_price * rush_multiplier",
      "example": "556.28 * 1.0 = 556.28"
    },
    "withAddOns": {
      "formula": "rush_price + (addon_price_per_unit * quantity)",
      "example": "556.28 + (0.15 * 100) = 571.28"
    },
    "volumeDiscounted": {
      "formula": "with_addons * (1 - volume_discount)",
      "example": "571.28 * (1 - 0.0) = 571.28"
    },
    "finalRetailPrice": {
      "formula": "volume_discounted * (1 + profit_margin)",
      "example": "571.28 * 1.35 = 771.23"
    }
  },
